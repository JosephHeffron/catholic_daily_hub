# Environment variables for Catholic Daily Hub
# Copy to .env.local and fill as needed

# Rate limiting (disable by leaving empty). Examples: "100/m", "200/60s"
RATE_LIMIT=

# Optional Redis for caching/limiting (Upstash URL)
REDIS_URL=

# Data sources
SOURCE_LITURGICAL_API_URL=
SOURCE_READINGS_BASE_URL=https://bible.usccb.org
SOURCE_SAINTS_BASE_URL=
SOURCE_HOURS_BASE_URL=https://universalis.com

# Optional error tracking
SENTRY_DSN=
